type Box<T> {
    value: T
}

type Pair<A, B> {
    left: A,
    right: B
}

fn first_right(p: Pair<number, number>) -> number {
    return p->right;
}

fn main(args: [String]) -> number {
    let box: Box<number> = { value = 10 };
    let pair: Pair<number, number> = { left = box->value, right = 32 };
    return first_right(pair);
}
