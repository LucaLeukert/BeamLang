fn divide(a: number, b: number) -> number!String {
    if (b == 0) {
        return!err "Division by zero";
    }
    return!ok a / b;
}

type User {
    id: number,
    name: String
}

fn main() -> number {
    println(typeof(10));
    println(typeof("hi"));

    let res = divide(1, 0);
    println(typeof(res));

    let user: User = { id = 1, name = "Ada" };
    println(typeof(user));

    return 0;
}
